<style>
.whatsapp-icon {
  bottom: {{ settings.bottom_range }}px;
  {{ settings.vertical_alignment }}: {{ settings.right_range }}px;
  width: {{ settings.icon_width }}px;
  height: {{ settings.icon_width }}px;
  {% if settings.whatsapp_icon_style == 'blend' %}
  mix-blend-mode: difference;
  {% endif %}
}

  @media (max-width: 768px) {
    .whatsapp-icon {
        bottom: {{ settings.bottom_range }}px;
        {{ settings.vertical_alignment }}: {{ settings.right_range }}px;
        width: {{ settings.mobile_icon_width }}px;
        height: {{ settings.mobile_icon_width }}px;
    }
  }
</style>

